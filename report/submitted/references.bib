@article{gezari2017,
  doi = {10.3847/1538-4357/835/2/144},
  url = {https://doi.org/10.3847%2F1538-4357%2F835%2F2%2F144},
  year = 2017,
  month = {jan},
  publisher = {American Astronomical Society},
  volume = {835},
  number = {2},
  pages = {144},
  author = {S. Gezari and T. Hung and S. B. Cenko and N. Blagorodnova and Lin Yan and S. R. Kulkarni and K. Mooley and A. K. H. Kong and T. M. Cantwell and P. C. Yu and Y. Cao and C. Fremling and J. D. Neill and C.-C. Ngeow and P. E. Nugent and P. Wozniak},
  title = {{iPTF} Discovery of the Rapid {\textquotedblleft}Turn-on{\textquotedblright} of a Luminous Quasar},
  journal = {The Astrophysical Journal},
  abstract = {We present a radio-quiet quasar at z = 0.237 discovered “turning on” by the intermediate Palomar Transient Factory (iPTF). The transient, iPTF 16bco, was detected by iPTF in the nucleus of a galaxy with an archival Sloan Digital Sky Survey spectrum with weak narrow-line emission characteristic of a low-ionization nuclear emission-line region (LINER). Our follow-up spectra show the dramatic appearance of broad Balmer lines and a power-law continuum characteristic of a luminous ( erg s−1) type 1 quasar 12 yr later. Our photometric monitoring with PTF from 2009–2012 and serendipitous X-ray observations from the XMM-Newton Slew Survey in 2011 and 2015 constrain the change of state to have occurred less than 500 days before the iPTF detection. An enhanced broad Hα/[O iii] λ5007 line ratio in the type 1 state relative to other changing-look quasars also is suggestive of the most rapid change of state yet observed in a quasar. We argue that the >10 increase in Eddington ratio inferred from the brightening in UV and X-ray continuum flux is more likely due to an intrinsic change in the accretion rate of a preexisting accretion disk than an external mechanism such as variable obscuration, microlensing, or the tidal disruption of a star. However, further monitoring will be helpful in better constraining the mechanism driving this change of state. The rapid “turn-on” of the quasar is much shorter than the viscous infall timescale of an accretion disk and requires a disk instability that can develop around a  black hole on timescales less than 1 yr.}
}


@article{yang2018,
  doi = {10.3847/1538-4357/aaca3a},
  url = {https://doi.org/10.3847%2F1538-4357%2Faaca3a},
  year = 2018,
  month = {jul},
  publisher = {American Astronomical Society},
  volume = {862},
  number = {2},
  pages = {109},
  author = {Qian Yang and Xue-Bing Wu and Xiaohui Fan and Linhua Jiang and Ian McGreer and Jinyi Shangguan and Su Yao and Bingquan Wang and Ravi Joshi and Richard Green and Feige Wang and Xiaotong Feng and Yuming Fu and Jinyi Yang and Yuanqi Liu},
  title = {Discovery of 21 New Changing-look {AGNs} in the Northern Sky},
  journal = {The Astrophysical Journal},
  abstract = {The rare case of changing-look (CL) active galactic nuclei (AGNs), with the appearance or disappearance of broad Balmer emission lines within a few years, challenges our understanding of the AGN unified model. We present a sample of 21 new CL AGNs at , which doubles the number of such objects known to date. These new CL AGNs were discovered in various ways, from (1) repeat spectra in the SDSS, (2) repeat spectra in the Large Sky Area Multi-Object Fiber Spectroscopic Telescope (LAMOST) and SDSS, and (3) photometric variability and new spectroscopic observations. We use the photometric data from surveys, including the SDSS imaging survey, the Pan-STARRS1, the DESI Legacy imaging survey, the Wide-field Infrared Survey Explorer (WISE), the Catalina Real-time Transient Survey, and the Palomar Transient Factory. The estimated upper limits of the transition timescale of the CL AGNs in this sample spans from 0.9 to 13 years in the rest-frame. The continuum flux in the optical and mid-infrared becomes brighter when the CL AGNs turn on, or vice versa. Variations of more than 0.2 mag in the W1 band were detected in 15 CL AGNs during the transition. The optical and mid-infrared variability is not consistent with the scenario of variable obscuration in 10 CL AGNs at more than the 3σ confidence level. We confirm a bluer-when-brighter trend in the optical. However, the mid-infrared WISE colors W1–W2 become redder when the objects become brighter in the W1 band, possibly due to a stronger hot dust contribution in the W2 band when the AGN activity becomes stronger. The physical mechanism of type transition is important for understanding the evolution of AGNs.}
}


