@ARTICLE{marziani2018,
  
AUTHOR={Marziani, Paola and Dultzin, Deborah and Sulentic, Jack W. and Del Olmo, Ascensión and Negrete, C. A. and Martínez-Aldama, Mary L. and D'Onofrio, Mauro and Bon, Edi and Bon, Natasa and Stirpe, Giovanna M.},   
TITLE={A Main Sequence for Quasars},      
