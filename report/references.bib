@article{ferrarese2000,
  doi = {10.1086/312838},
  url = {https://doi.org/10.1086%2F312838},
  year = 2000,
  month = {aug},
  publisher = {{IOP} Publishing},
  volume = {539},
  number = {1},
  pages = {L9--L12},
  author = {Laura Ferrarese and David Merritt},
  title = {A Fundamental Relation between Supermassive Black Holes and Their Host Galaxies},
  journal = {The Astrophysical Journal},
  abstract = {The masses of supermassive black holes correlate almost perfectly with the velocity dispersions of their host bulges, Mbh ∝ σα, where α = 4.8 ± 0.5. The relation is much tighter than the relation between Mbh and bulge luminosity, with a scatter no larger than expected on the basis of measurement error alone. Black hole masses recently estimated by Magorrian et al. lie systematically above the Mbh-σ relation defined by more accurate mass estimates, some by as much as 2 orders of magnitude. The tightness of the Mbh-σ relation implies a strong link between black hole formation and the properties of the stellar bulge.}
}


